{
    "name": "Sentinel V2",
    "version": "2.0",
    "manifest_version": 3,
    "description": "Phishing & Malware Analysis System",
    "permissions": [
        "nativeMessaging",
        "declarativeNetRequest",
        "scripting",
        "offscreen",
        "webNavigation",
        "notifications",
        "downloads",
        "storage",
        "tabs"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    }
}